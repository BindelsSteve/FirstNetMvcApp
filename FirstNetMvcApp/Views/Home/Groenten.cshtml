
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <title>Groenten</title>
</head>
<body>

    <h2>Groenten</h2>

    <ul>
        @foreach(string groente in ViewBag.Groenten)
        {
            <li>@groente</li>
        }
    </ul>

    @ViewBag.Zoekresultaat
</body>
</html>


